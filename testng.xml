<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="CrossBrowserSuite">
    <test name="CrossBrowserTest1">
        <parameter name="browser" value="chrome" />
        <classes>
            <class name="crossbrowsing.CrossBrowsing" />
        </classes>
    </test>
    <test name="CrossBrowserTest2">
        <parameter name="browser" value="edge">
            <classes>
                <class name="crossbrowsing.CrossBrowsing"></class>
            </classes>
        </parameter>
    </test>
</suite>
